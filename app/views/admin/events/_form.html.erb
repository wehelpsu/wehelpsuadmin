<%= semantic_form_for(@event,:url=>admin_funds_url) do |f| %>
  <%= f.inputs do %>
    <%= f.input :place %>
    <%= f.input :date %>
    <%= f.input :budget %>
    <%= f.input :user_id %>
    <%= f.input :status %>
  <% end %>

  <%= f.actions do %>
    <%= f.action :submit, :as => :input %>
  <% end %>
<% end %>
