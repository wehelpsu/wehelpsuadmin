<h1>Listing admin_expences</h1>

<table>
  <tr>
    <th>Expence name</th>
    <th>Expence date</th>
    <th>Amount</th>
    <th>Given by name</th>
    <th>Bill no</th>
    <th>Place of purchase</th>
    <th>Updated user</th>
    <th>Updated user type</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @admin_expences.each do |admin_expence| %>
  <tr>
    <td><%= admin_expence.expence_name %></td>
    <td><%= admin_expence.expence_date %></td>
    <td><%= admin_expence.amount %></td>
    <td><%= admin_expence.given_by_name %></td>
    <td><%= admin_expence.bill_no %></td>
    <td><%= admin_expence.place_of_purchase %></td>
    <td><%= admin_expence.updated_user_id %></td>
    <td><%= admin_expence.updated_user_type %></td>
    <td><%= link_to 'Show', admin_expence %></td>
    <td><%= link_to 'Edit', edit_admin_expence_path(admin_expence) %></td>
    <td><%= link_to 'Destroy', admin_expence, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Expence', new_admin_expence_path %>
